@using AutoRest.CSharp.Templates
@inherits AutoRest.Core.Template<AutoRest.CSharp.TemplateModels.MethodGroupTemplateModel>
@Header("// ")
@EmptyLine
namespace @Settings.Namespace
{
    using System.Collections.Generic;
    using System.Threading.Tasks;
    using Criteo.Platform.Shared;
    using System.Globalization;
    using Microsoft.Practices.Unity;

@foreach (var usingString in Model.Usings) {
    @:using @usingString;
}
@EmptyLine
    public class @(Model.MethodGroupType) : I@(Model.MethodGroupType)
    {
        private IHttpClient _client;
	    private IServiceUriBuilder _uriBuilder;

        public @(Model.MethodGroupType)(IHttpClient client, [Dependency("@(Model.MethodGroupType)")] IServiceUriBuilder uriBuilder)
	    {
	      _client = client;
		  _uriBuilder = uriBuilder;
        }

        @foreach (var method in Model.MethodTemplateModels)
        {
        @:@(Include(new MethodTemplate(), method))
        @EmptyLine
        }
    }
}
