@using System
@using System.Linq;
@using AutoRest.Core.Utilities
@inherits AutoRest.Core.Template<AutoRest.CSharp.TemplateModels.MethodGroupTemplateModel>
@Header("// ")
@EmptyLine
namespace @Settings.Namespace
{
    using System.Collections.Generic;
    using System.Threading.Tasks;
@foreach (var usingString in Model.Usings) {
    @:using @usingString;
}
@EmptyLine
    public interface I@(Model.MethodGroupType)
    {
    @foreach(var method in Model.MethodTemplateModels)
    {
        @:@(method.TaskExtensionReturnTypeString) @(method.Name)(@(method.GetAsyncMethodParameterDeclaration(true)));
    }
    }
}
